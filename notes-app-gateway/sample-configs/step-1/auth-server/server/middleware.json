{
  "initial:before": {
    "loopback#favicon": {
    },
    "cors": {
    },
    "strong-express-metrics": {
    }
  },
  "initial": {
    "compression": {
      "params": {
        "threshold": "4kb"
      }
    }
  },
  "session": {
    "express-session": {
      "params": {
        "saveUninitialized": true,
        "resave": true,
        "secret": "keyboard cat"
      }
    }
  },
  "auth": {
  },
  "routes:before": {
    "./middleware/https-redirect": {
      "params": {
        "httpsPort": 3202
      }
    }
  },
  "routes:after": {
    "./middleware/proxy": {
      "params": {
        "rules": [
          "^/api/(.*)$ http://localhost:3001/api/$1 [p]"
        ]
      }
    }
  },
  "files": {
    "loopback#static": [
      {
        "params": "$!../client/public",
        "paths": ["/"]
      },
      {
        "params": "$!../client/admin",
        "paths": ["/admin"]
      }
    ]
  },
  "final": {
    "loopback#url-not-found": {}
  },
  "final:after": {
    "errorhandler": {"params": {"log": false}}
  }
}
